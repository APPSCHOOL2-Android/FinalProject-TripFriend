<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        android:elevation="30dp"
        app:cardBackgroundColor="@android:color/white"
        app:cardCornerRadius="8dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:padding="10dp">

                <ImageView
                    android:id="@+id/imageViewReviewUserProfileImage"
                    android:layout_width="96dp"
                    android:layout_height="87dp"
                    app:srcCompat="@drawable/person_24px" />

                <TextView
                    android:id="@+id/textViewReviewUserNickname"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="TextView"
                    android:textAppearance="@style/TextAppearance.AppCompat.Large"
                    android:textColor="#000000" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:padding="10dp">

                <TextView
                    android:id="@+id/textView9090"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="10dp"
                    android:text="여행 점수"
                    android:textAppearance="@style/TextAppearance.AppCompat.Medium" />

                <SeekBar
                    android:id="@+id/seekBarReviewScore"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:max="10"
                    android:progress="0" />

                <TextView
                    android:id="@+id/textViewReviewScore"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="end"
                    android:layout_marginBottom="15dp"
                    android:text="0/10" />

                <TextView
                    android:id="@+id/textView7567"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="여행 스타일"
                    android:textAppearance="@style/TextAppearance.AppCompat.Medium" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">
                        <com.google.android.material.chip.ChipGroup
                            android:id="@+id/chipGroup1"
                            android:layout_width="wrap_content"
                            app:singleSelection="true"
                            android:layout_height="wrap_content">

                            <com.google.android.material.chip.Chip
                                android:id="@+id/chipReview1"
                                style="@style/Colors_Widget.MaterialComponents.Chip.Choice"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="0dp"
                                android:layout_marginRight="5dp"
                                android:checkable="true"
                                android:text="계획적인"
                                app:checkedIconVisible="false"
                                app:chipIconVisible="false"
                                app:chipStrokeColor="@color/highLightColor"
                                app:chipStrokeWidth="1dp"
                                app:rippleColor="@null" />

                            <com.google.android.material.chip.Chip
                                android:id="@+id/chipReview2"
                                style="@style/Colors_Widget.MaterialComponents.Chip.Choice"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="0dp"
                                android:layout_marginRight="5dp"
                                android:checkable="true"
                                android:text="즉흥적인"
                                app:checkedIconVisible="false"
                                app:chipIconVisible="false"
                                app:chipStrokeColor="@color/highLightColor"
                                app:chipStrokeWidth="1dp"
                                app:rippleColor="@null" />

                        </com.google.android.material.chip.ChipGroup>


                        <com.google.android.material.chip.ChipGroup
                            android:id="@+id/chipGroup2"
                            app:singleSelection="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <com.google.android.material.chip.Chip
                                android:id="@+id/chipReview3"
                                style="@style/Colors_Widget.MaterialComponents.Chip.Choice"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="0dp"
                                android:layout_marginRight="5dp"
                                android:checkable="true"
                                android:text="부지런한"
                                app:checkedIconVisible="false"
                                app:chipIconVisible="false"
                                app:chipStrokeColor="@color/highLightColor"
                                app:chipStrokeWidth="1dp"
                                app:rippleColor="@null" />

                            <com.google.android.material.chip.Chip
                                android:id="@+id/chipReview4"
                                style="@style/Colors_Widget.MaterialComponents.Chip.Choice"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="0dp"
                                android:layout_marginRight="1dp"
                                android:checkable="true"
                                android:text="게으른"
                                app:checkedIconVisible="false"
                                app:chipIconVisible="false"
                                app:chipStrokeColor="@color/highLightColor"
                                app:chipStrokeWidth="1dp"
                                app:rippleColor="@null" />


                        </com.google.android.material.chip.ChipGroup>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <com.google.android.material.chip.ChipGroup
                            android:id="@+id/chipGroup3"
                            app:singleSelection="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <com.google.android.material.chip.Chip
                                android:id="@+id/chipReview5"
                                style="@style/Colors_Widget.MaterialComponents.Chip.Choice"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="0dp"
                                android:layout_marginRight="5dp"
                                android:checkable="true"
                                android:text="적극적인"
                                app:checkedIconVisible="false"
                                app:chipIconVisible="false"
                                app:chipStrokeColor="@color/highLightColor"
                                app:chipStrokeWidth="1dp"
                                app:rippleColor="@null" />

                            <com.google.android.material.chip.Chip
                                android:id="@+id/chipReview6"
                                style="@style/Colors_Widget.MaterialComponents.Chip.Choice"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="0dp"
                                android:layout_marginRight="5dp"
                                android:checkable="true"
                                android:text="소극적인"
                                app:checkedIconVisible="false"
                                app:chipIconVisible="false"
                                app:chipStrokeColor="@color/highLightColor"
                                app:chipStrokeWidth="1dp"
                                app:rippleColor="@null" />

                        </com.google.android.material.chip.ChipGroup>

                        <com.google.android.material.chip.ChipGroup
                            android:id="@+id/chipGroup4"
                            app:singleSelection="true"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <com.google.android.material.chip.Chip
                                android:id="@+id/chipReview7"
                                style="@style/Colors_Widget.MaterialComponents.Chip.Choice"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="0dp"
                                android:layout_marginRight="5dp"
                                android:checkable="true"
                                android:text="깔끔한"
                                app:checkedIconVisible="false"
                                app:chipIconVisible="false"
                                app:chipStrokeColor="@color/highLightColor"
                                app:chipStrokeWidth="1dp"
                                app:rippleColor="@null" />

                            <com.google.android.material.chip.Chip
                                android:id="@+id/chipReview8"
                                style="@style/Colors_Widget.MaterialComponents.Chip.Choice"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="0dp"
                                android:layout_marginRight="1dp"
                                android:checkable="true"
                                android:text="지저분한"
                                app:checkedIconVisible="false"
                                app:chipIconVisible="false"
                                app:chipStrokeColor="@color/highLightColor"
                                app:chipStrokeWidth="1dp"
                                app:rippleColor="@null" />

                        </com.google.android.material.chip.ChipGroup>



                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <com.google.android.material.chip.ChipGroup
                            android:id="@+id/chipGroup5"
                            app:singleSelection="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <com.google.android.material.chip.Chip
                                android:id="@+id/chipReview9"
                                style="@style/Colors_Widget.MaterialComponents.Chip.Choice"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="0dp"
                                android:layout_marginRight="5dp"
                                android:checkable="true"
                                android:text="시끄러운"
                                app:checkedIconVisible="false"
                                app:chipIconVisible="false"
                                app:chipStrokeColor="@color/highLightColor"
                                app:chipStrokeWidth="1dp"
                                app:rippleColor="@null" />

                            <com.google.android.material.chip.Chip
                                android:id="@+id/chipReview10"
                                style="@style/Colors_Widget.MaterialComponents.Chip.Choice"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="0dp"
                                android:layout_marginRight="1dp"
                                android:checkable="true"
                                android:text="조용한"
                                app:checkedIconVisible="false"
                                app:chipIconVisible="false"
                                app:chipStrokeColor="@color/highLightColor"
                                app:chipStrokeWidth="1dp"
                                app:rippleColor="@null" />

                        </com.google.android.material.chip.ChipGroup>


                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

    </androidx.cardview.widget.CardView>
</LinearLayout>